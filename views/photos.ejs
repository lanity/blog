<table class="table">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">Thumbnail</th>
        <th scope="col">Categories</th>
        <th scope="col">Name</th>
        <th scope="col">Action</th>
      </tr>
    </thead>
    <tbody>
      <% let count = 1; %>
      <% photos.forEach(photo => { %>
      <tr>
        <th scope="row"><%=count++%></th>
        <td><img src="/<%= photo.filename %>" style="width: 100px; height: 100px;" class="img-thumbnail" alt="<%= photo.originalname %>"></td>
        <td>Otto</td>
        <td><%= photo.originalname %></td>
        <td>edit / <a href="/admin/delete-photo/<%= photo._id %>">delete</a></td>
      </tr>
      <% }) %>
    </tbody>
  </table>



